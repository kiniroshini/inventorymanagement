<% for(var i = 0; i < items.length; i++) { %>

<tr>
    <td><%= i + 1 %></td>
    <td><%= items[i].name %></td>
    <td><%= items[i].description %></td>
    <td><%= items[i].category %></td>
    <td><%= items[i].availability %></td>
    <td><a href="/update-user?id=<%=items[i]._id%>" class="btn border-shadow update"><span class="text-gradient"><i class="fas fa-pencil-alt"></i></span></a>
    <a class="btn border-shadow delete" data-id = <%= items[i]._id%> >
        <span class="text-gradient"><i class="fas fa-times"></i></span></a></td>
    </tr>
<% } %>